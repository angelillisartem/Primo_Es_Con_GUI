<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.first.esgui.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label style="-fx-font-size: 18; -fx-font-style: Italic;" text="Login" />
    <Label />
    <Label text="Username" />
    <TextField prefHeight="25.0" prefWidth="86.0" />
    <Label text="Password" />
    <PasswordField prefHeight="25.0" prefWidth="97.0" />
    <Label fx:id="Login" />
    <Label fx:id="Register" />
    <Button onAction="#onLoginButtonClick" prefHeight="25.0" prefWidth="64.0" text="Login" />
    <Button onAction="#onRegisterButtonClick" prefHeight="25.0" prefWidth="64.0" text="Register" />
    <ChoiceBox onAction="#onChoiceBoxClick"> </ChoiceBox>

</HBox>